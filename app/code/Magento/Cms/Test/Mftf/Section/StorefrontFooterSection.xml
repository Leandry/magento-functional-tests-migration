<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="StorefrontFooterSection">
        <element name="footerLinks" type="text" selector="ul.footer.links" />
        <element name="footerLinkByText" type="text" selector="//ul[contains(@class, 'footer') and contains(@class, 'links')]/li/a[contains(text(),'{{linkName}}')] "
                 parameterized="true" />
    </section>
</sections>
